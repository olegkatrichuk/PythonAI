# Production Environment Variables
# ВАЖНО: Замените эти значения на реальные для production!

# Настройки базы данных
DATABASE_URL=postgresql://postgres:CHANGE_THIS_PASSWORD@pg:5432/aifinder_db

# Секретный ключ для JWT токенов (ОБЯЗАТЕЛЬНО ЗАМЕНИТЕ НА НОВЫЙ!)
SECRET_KEY=CHANGE_THIS_TO_SECURE_RANDOM_STRING_IN_PRODUCTION

# Алгоритм подписи токенов
ALGORITHM=HS256

# Время жизни токена в минутах
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Ваш ключ от Gemini API
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# URL Frontend приложения
FRONTEND_URL=https://yourdomain.com

# Redis URL для кэширования
REDIS_URL=redis://redis:6379/0

# Окружение
ENVIRONMENT=production